{
    "name": "nest-init-server-kit",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build:dev": "NODE_ENV=development nest build",
        "build:prod": "NODE_ENV=production nest build",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" ",
        "start": "NODE_ENV=production DEBUG=app-*,http-* nest start",
        "start:dev": "NODE_ENV=development DEBUG=app-*,http-* nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "NODE_ENV=production DEBUG=app-*,http-* node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "NODE_ENV=test jest --runInBand  --forceExit",
        "test:watch": "NODE_ENV=test  jest --watch --runInBand ",
        "test:cov": "NODE_ENV=test  jest --coverage --runInBand  --forceExit",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "NODE_ENV=testing jest --config ./test/jest-e2e.json",
        "init:db": "bash ./scripts/db.sh"
    },
    "dependencies": {
        "@nestjs/axios": "^0.0.8",
        "@nestjs/common": "^8.4.3",
        "@nestjs/config": "^2.0.0",
        "@nestjs/core": "^8.4.3",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mapped-types": "^1.0.1",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.4.3",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/typeorm": "^8.0.3",
        "@sendgrid/mail": "^7.6.2",
        "aws-sdk": "^2.1136.0",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "compression": "^1.7.4",
        "cookie": "^0.4.1",
        "cookie-parser": "^1.4.5",
        "crypto-js": "^4.1.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "helmet": "^5.0.2",
        "http-status-codes": "^2.2.0",
        "joi": "17.6.0",
        "joi-message": "^1.1.0",
        "joi-password": "^3.0.1",
        "moment": "^2.29.3",
        "mono-utils-core": "^0.10.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "mysql2": "^2.3.3",
        "passport": "^0.5.2",
        "passport-facebook": "^3.0.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "swagger-tools": "^0.10.4",
        "swagger-ui-express": "^4.3.0",
        "typeorm": "^0.2.41"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.5.1",
        "@nestjs/schematics": "^7.1.3",
        "@nestjs/testing": "8.0.6",
        "@types/aws-sdk": "^2.7.0",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/helmet": "^4.0.0",
        "@types/i18n": "^0.13.1",
        "@types/jest": "^26.0.24",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.0.0",
        "@types/passport-facebook": "^2.1.11",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.5.1",
        "jest-ts-auto-mock": "^2.1.0",
        "prettier": "^2.3.2",
        "supertest": "^6.1.3",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "ttypescript": "^1.5.13",
        "typescript": "^4.6.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
